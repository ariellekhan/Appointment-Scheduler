<!DOCTYPE ui:UiBinder SYSTEM "http://dl.google.com/gwt/DTD/xhtml.ent">
<ui:UiBinder xmlns:ui="urn:ui:com.google.gwt.uibinder"
	xmlns:g="urn:import:com.google.gwt.user.client.ui">
	<ui:style>
	.important {
		font-weight: bold;
	}
	</ui:style>

	<g:HTMLPanel>

		<g:VerticalPanel ui:field="mainVPanel">
			<g:HorizontalPanel ui:field="mainHPanel">
				<g:VerticalPanel ui:field= "dateVPanel">
				<g:HorizontalPanel><g:Label>Select Date: </g:Label><g:Label ui:field="appointmentDateLabel"></g:Label> </g:HorizontalPanel>
					
				</g:VerticalPanel>		
				
				<g:VerticalPanel ui:field="timeVPanel">
					<g:Label ui:field="timeLabel">Select Time</g:Label>
					
				</g:VerticalPanel>
			

			</g:HorizontalPanel>
			<g:Button ui:field="submitButton">Book Appointment</g:Button>
		
		</g:VerticalPanel>

		
		
	
	</g:HTMLPanel>

</ui:UiBinder> 